# Musk Configuration Example
# ==========================
# 
# This example shows a typical multi-environment setup.
# Copy to musk.conf and customize for your setup.
#
# Select the active environment with MUSK_ENV (defaults to "dev").
#
# Network options:
#   - "regtest"   : Local development/testing (default port: 18884)
#   - "testnet"   : Liquid testnet (default port: 18892)
#   - "liquidv1"  : Liquid mainnet (default port: 7041)

# =============================================================================
# Development Environment - Local regtest node
# =============================================================================
[dev]
network = "regtest"
url = "http://127.0.0.1:18884"
wallet = "musk_dev"
user = "user"
password = "password"
# genesis_hash = "varies-per-regtest-chain"

# =============================================================================
# Test Environment - Liquid testnet
# =============================================================================
[test]
network = "testnet"
url = "http://127.0.0.1:18892"
wallet = "musk_test"
user = "elements"
password = "elementspass"
genesis_hash = "a771d8a8e75d8fa1b9bc08af6a5bde4f4fb4d7f01bd8b4b36e7cd97fb4b3d4e3"

# =============================================================================
# Production Environment - Liquid mainnet
# =============================================================================
[prod]
network = "liquidv1"
url = "http://127.0.0.1:7041"
wallet = "musk_prod"
user = "your-mainnet-user"
password = "your-mainnet-password"
genesis_hash = "1466275836220db2944ca059a3a10ef6fd2ea684b0688d2c379296888a206003"
